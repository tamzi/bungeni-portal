/* Updated tab styles */
#content ul.tabs {
    list-style: none;
    margin: 0;
    border: none;
}

#content ul.tabs li.wysiwygTab-selector {
    float: left;
    margin: 0.5em 10px 0 0 !important;;
    position: relative;
    padding: 0;
}

#content ul.tabs a {
    display:block;
    color:#fff;
    text-decoration:none;
    padding:0 15px;
    line-height:25px;
    height:25px;
    font-weight:normal;
    font-family: Arial,Helvetica,sans-serif;
    background:#464646;
    border:1px solid #000;
    border-bottom:none;
    -webkit-background-clip: padding-box;
    -webkit-font-smoothing:antialiased;
    -webkit-border-top-left-radius:8px;
    -webkit-border-top-right-radius:8px;
    -moz-border-radius:8px 8px 0 0;
    border-top-left-radius:8px;
    border-top-right-radius:8px;
    text-shadow: none;
    position:relative;
}

#content ul.tabs li.wysiwygTab-selector:before,
#content ul.tabs li.wysiwygTab-selector:after {
    content:'';
    width:9px;
    height:0px;
    position:absolute;
    z-index:2;
    bottom:0;
    background:#464646;
}

#content ul.tabs li.wysiwygTab-selector:before {
    left:-8px;
}

#content ul.tabs li.wysiwygTab-selector:after {
    right:-8px;
}

#content ul.tabs li.current a {
    color: #000000;
    background: #F9AC32;
    font-family: Arial,Helvetica,sans-serif;
    text-shadow: none;
}

#content ul.tabs a:before, #content ul.tabs a:after {
    content:'';
    width:10px;
    height:0px;
    position:absolute;
    z-index:3;
    bottom:-1px;
    background:#434343;
    overflow:hidden;
    /*border-bottom:1px solid #333;*/
    border-bottom: none;
    margin-top:-26px;
    -webkit-background-clip: padding-box;
}

#content ul.tabs a:before {
    left:-11px;
    border-bottom-right-radius:8px;
    -webkit-border-bottom-right-radius:8px;
    -moz-border-radius-bottomright:8px;
    border-right:1px solid #222;
}

#content ul.tabs a:after {
    right:-11px;
    border-bottom-left-radius:8px;
    -webkit-border-bottom-left-radius:8px;
    -moz-border-radius-bottomleft:8px;
    border-left:1px solid #222;
}

#content ul.tabs li.selected a {
    background:#7e7e7e;
    color:#fff;
    height:26px;
    text-shadow:#222 0 1px 0;
    -webkit-background-clip: padding-box;
}

#content ul.tabs li.selected a:before,
#content ul.tabs li.selected a:after {
    bottom:0;
}

#content ul.tabs li.selected:before,
#content ul.tabs li.selected:after {
    background:#7e7e7e;
    bottom:0;
    -webkit-background-clip: padding-box;
}

#content div.pane-container div.pane {
    border-top: 1px solid #999999;
    clear: both;
}

