<workspace id="bill">
    <state id="draft">
        <tab id="draft" roles="bungeni.Owner"/>
    </state>
    <state id="working_draft">
        <tab id="draft" roles="bungeni.Clerk"/>
    </state>
    <state id="withdrawn_internal">
        <tab id="archive" roles="bungeni.Owner"/>
    </state>
    <state id="withdrawn_external">
        <tab id="archive" roles="bungeni.Owner bungeni.Clerk bungeni.Speaker"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner"/>
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="bungeni.Clerk bungeni.Speaker bungeni.Signatory"/>
    </state>
    <state id="received">
        <tab id="inbox" roles="bungeni.Clerk"/>
        <tab id="sent" roles="bungeni.Owner"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="bungeni.Owner"/>
    </state>
    <state id="completed">
        <tab id="inbox" roles="bungeni.Speaker"/>
    </state>
    <state id="incomplete">
        <tab id="inbox" roles="bungeni.Speaker"/>
    </state>
     <state id="inadmissible">
        <tab id="inbox" roles="bungeni.Speaker"/>
    </state>
    <state id="admissible">
        <tab id="inbox" roles="bungeni.Speaker"/>
    </state>
    <state id="schedule_pending">
        <tab id="inbox" roles="bungeni.Speaker"/>
    </state>
    <state id="adjourned">
        <tab id="archive" roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="deferred">
        <tab id="archive" roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="elapsed">
        <tab id="archive" roles="bungeni.Clerk bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="dropped">
        <tab id="archive" roles="bungeni.Speaker bungeni.Owner"/>
    </state>
    <state id="withdrawn">
        <tab id="archive" roles="bungeni.Owner bungeni.Clerk bungeni.Speaker"/>
    </state>
</workspace>
