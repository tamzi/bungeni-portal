<workspace id="motion">
<!-- !+ this conf file format is INVALID XML, because:
"it is an error if distinct elements in the same document are defining the same identifier"
http://en.wikipedia.org/wiki/Document_Type_Definition
-->
    <state id="working_draft">
        <tab id="draft" roles="ClerkSenate ClerkAssembly" />
    </state>
    <state id="draft">
        <tab id="draft" roles="Owner" />
    </state>
    <state id="redraft">
        <tab id="draft" roles="Owner" />
    </state>
    <state id="withdrawn_signatory">
        <tab id="archive" 
             roles="Owner ClerkSenate ClerkAssembly Signatory" />
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly" />
        <tab id="pending" roles="Owner Signatory" />
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="Signatory" />
        <tab id="pending" roles="Owner" />
    </state>
    <state id="received">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly CommitteeMember" />
        <tab id="pending" roles="Owner Signatory" />
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="Owner" />
        <tab id="pending" roles="ClerkSenate ClerkAssembly Signatory" />
    </state>
    <state id="reviewed">
        <tab id="inbox" roles="SpeakerSenate SpeakerAssembly" />
        <tab id="pending" 
            roles="Owner ClerkSenate ClerkAssembly Signatory" />
    </state>
    <state id="incomplete">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly" />
        <tab id="pending" roles="Owner Signatory" />
    </state>
    <state id="withdrawn_internal">
        <tab id="archive" 
            roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
     <state id="admissible">
        <tab id="inbox" roles="ClerkSenate ClerkAssembly" />
        <tab id="pending"
	        roles="Owner Signatory SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="schedule_pending">
        <tab id="pending" 
	        roles="Owner Signatory SpeakerSenate SpeakerAssembly ClerkSenate ClerkAssembly" />
    </state>
    <state id="adopted">
        <tab id="archive" 
            roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="adopted_amendments">
         <tab id="archive" 
            roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="adjourned">
         <tab id="archive" 
	        roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="deferred">
        <tab id="archive" 
	        roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="elapsed">
        <tab id="archive" 
            roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="dropped">
        <tab id="archive" 
	        roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="rejected">
        <tab id="archive"
            roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>
    <state id="withdrawn">
        <tab id="archive"
	        roles="Signatory Owner ClerkSenate ClerkAssembly SpeakerSenate SpeakerAssembly" />
    </state>

</workspace>

