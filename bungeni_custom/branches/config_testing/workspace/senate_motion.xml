<workspace id="motion">
    <state id="draft">
        <tab id="draft" roles="Drafter" />
    </state>
    <state id="redraft">
        <tab id="draft" roles="Drafter" />
    </state>
    <state id="submitted_signatories">
        <tab id="inbox" roles="Signatory"/>
        <tab id="pending" roles="Drafter Owner"/>
    </state>
    <state id="withdrawn_signatory">
        <tab id="archive" roles="Owner ClerkSenate Signatory"/>
    </state>
    <state id="submitted">
        <tab id="inbox" roles="ClerkSenate MemberCommitteeSenate  "/>
        <tab id="pending" roles="Owner Signatory"/>
    </state>
    <state id="clarification_required">
        <tab id="inbox" roles="Owner"/>
        <tab id="pending" roles="  ClerkSenate Signatory"/>
    </state>
    <state id="reviewed">
        <tab id="inbox" roles="  SpeakerSenate"/>
        <tab id="pending" 
	     roles="Owner
		      ClerkSenate 
		    Signatory"/>
    </state>
    <state id="withdrawn_internal">
        <tab id="archive" 
	     roles="Signatory
		    Owner
		      ClerkSenate
                      SpeakerSenate"/>
    </state>
     <state id="admissible">
        <tab id="schedule" roles="  ClerkSenate"/>
        <tab id="pending"
	     roles="Owner
		    Signatory
		      SpeakerSenate"/>
    </state>
    <state id="scheduled">
        <tab id="schedule" roles="  ClerkSenate"/>
        <tab id="pending"
	     roles="Owner
		    Signatory
		      SpeakerSenate"/>
    </state>
    <state id="adopted">
        <tab id="archive" 
	     roles="Signatory 
		    Owner
		      ClerkSenate
          SpeakerSenate"/>
    </state>
    <state id="adopted_amendments">
         <tab id="archive" 
	      roles="Signatory
		     Owner
		       ClerkSenate
                       SpeakerSenate"/>
    </state>
    <state id="adjourned">
        <tab id="schedule" roles="  ClerkSenate"/>
        <tab id="pending"
	     roles="Owner
		    Signatory
		      SpeakerSenate"/>
    </state>
    <state id="deferred">
        <tab id="schedule" roles="  ClerkSenate"/>
        <tab id="pending"
	     roles="Owner
		    Signatory
		      SpeakerSenate"/>
    </state>
    <state id="elapsed">
        <tab id="archive" 
	     roles="Signatory
		    Owner
		      ClerkSenate
                      SpeakerSenate"/>
    </state>
    <state id="dropped">
        <tab id="archive" 
	     roles="Signatory 
		    Owner 
		      ClerkSenate
                      SpeakerSenate"/>
    </state>
    <state id="rejected">
         <tab id="archive"
	      roles="Signatory
		     Owner
		       ClerkSenate
                       SpeakerSenate"/>
    </state>
    <state id="withdrawn">
        <tab id="archive"
	     roles="Signatory
		    Owner
		      ClerkSenate
                      SpeakerSenate"/>
    </state>
</workspace>
