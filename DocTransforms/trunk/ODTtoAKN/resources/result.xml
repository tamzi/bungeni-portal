         <text:section text:style-name="Sect23" text:name="speech890297" BungeniSectionType="Speech" BungeniQuestionsContainerType="OralAnswersToQuestions" BungeniSpeechID="9" BungeniSpeechType="PointOfOrder" BungeniSpeechBy="userSpeaker">
            <p/>
            <text:section text:style-name="Sect24" text:name="heading722863" BungeniSectionType="Speech" BungeniQuestionsContainerType="OralAnswersToQuestions" BungeniSpeechID="9" BungeniSpeechType="Person">
               <p>
                  <text:reference-mark-start text:name="BungeniSpeechAs:9"/>
                  <eol/>
                  <text:reference-mark-end text:name="BungeniSpeechAs:9"/>
                  <eol/>
               </p>
            </text:section>
            <eol/>
            <p>Next Question, Dr. Kulundu!</p>
         </text:section>
         <eol/>
         <eol/>
         <eol/>
      </text:section>
