<?xml version="1.0"?>
<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser">
    
    <browser:resource name="transcripts.css" file="resources/css/transcripts.css" />
    <browser:resource name="no_media.png" file="resources/img/no_media.png" />
    <browser:resource name="control_play_blue.png" file="resources/img/control_play_blue.png" />
    <browser:resource name="transcripts.js" file="resources/js/transcripts.js" />
    <browser:resource name="ui.slider.js" file="resources/js/ui.slider.js" />
    <browser:resource name="mwEmbed-player-static.css" file="resources/css/mwEmbed-player-static.css" />
    <browser:resource name="mwEmbed-player-static.js" file="resources/js/mwEmbed-player-static.js" />
    <browser:resource name="jquery-1.4.2.min.js" file="resources/js/jquery-1.4.2.min.js" />
    <browser:resourceDirectory
        name="libraries"
        directory="resources/libraries"
    />
    <browser:resourceDirectory
        name="skins"
        directory="resources/skins"
    />
    <browser:resourceDirectory
        name="modules"
        directory="resources/modules"
    />
</configure>
