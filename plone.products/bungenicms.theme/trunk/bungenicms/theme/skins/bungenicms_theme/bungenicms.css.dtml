body {
  font: 69% Verdana, sans-serif;
  color: #000000;
  margin: 0;
  padding: 0;
  text-align:justify;
}

#portal-searchbox {
    padding: 0.5em 2em 0 0 !important;
}

.documentEditable {
    margin: 2.5em -1em 0em -1em !important;
}

#content {
    font-size: 125% !important;
    padding: 0 1em 2em 1em !important;
    background: White;
}

#content-core {
    border: 1px solid #C1C1C0 !important;
    min-height: 500 px !important;
    padding: 0.5em !important;
}

#content-core,
#content-core div p {
    text-align: justify;
}

#content-core h1, 
#content-core h2 {
    text-align: justify !important; 
    width: 100% !important
}

#content div.documentByLine {
